URxvt.letterSpace:      -1


URxvt*perl-lib:            /usr/lib/urxvt/perl/
URxvt*perl-ext-common:        default,matcher
URxvt.url-select.button:   1
URxvt.url-select.launcher: x-www-browser
URxvt*matcher.pattern.1:   \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
urxvt*urlLauncher: /usr/bin/google-chrome
urxvt*matcher.Button: 1

URxvt.clipboard.copycmd:  xsel --input  --clipboard
URxvt.clipboard.pastecmd: xsel --output --clipboard

URxvt.keysym.Control-Shift-C: perl:clipboard:copy
URxvt.keysym.Control-Shift-V: perl:clipboard:paste

!XFT
!Xft.lcdfilter : lcddefault
!Xft.hintstyle : hintslight
!Xft.hinting   : true
!Xft.antialias : true
!Xft.rgba      : rgb

!Fading
URxvt.fading    : 30
URxvt.fadeColor : #333333

URxvt.perl-ext-common: ...,font-size
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal

ssh-askpass*background: #000000
